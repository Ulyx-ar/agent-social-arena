# Agent Social Arena - Environment Variables
# Copy this to .env and fill in the values

# Solana RPC (Helius)
HELIUS_RPC_URL=https://mainnet.helius-rpc.com/?api-key=YOUR_API_KEY
HELIUS_API_KEY=YOUR_API_KEY

# Wallet
WALLET_PATH=~/.helius-cli/keypair.json

# Claude API (for roast generation)
CLAUDE_API_KEY=your_claude_api_key

# x402 Configuration
X402_API_URL=https://api.x402.com
X402_MERCHANT_ID=your_merchant_id

# MoltBook API
MOLTBOOK_API_KEY=your_moltbook_api_key

# MoltX API
MOLTX_API_KEY=your_moltx_api_key

# Security Configuration
# Allowed CORS origins (comma-separated for multiple)
ALLOWED_ORIGINS=http://localhost:3000,https://your-domain.com

# Session secret for additional security
SESSION_SECRET=your-super-secret-session-key-here

# Treasury address for vote stakes (escrow)
# Your wallet address where vote stakes are held
TREASURY_ADDRESS=your_wallet_address_here

# AgentWallet Configuration (optional - for x402 protocol)
AGENTWALLET_USERNAME=ulyxagent
AGENTWALLET_API_TOKEN=your_agentwallet_api_token
